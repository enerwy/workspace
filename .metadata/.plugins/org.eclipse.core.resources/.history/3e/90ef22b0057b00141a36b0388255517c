#!/usr/bin/python
# -*- coding: utf-8 -*-

import psycopg2
import sys


con = None

try:
     
    con = psycopg2.connect(database='asocreto', user='postgres', password= 'dsa') 
    cur = con.cursor()
    cur.execute('SELECT * from categoria')          
    ver = cur.fetchall()
    print ver
    

except psycopg2.DatabaseError, e:
    print 'Error %s' % e    
    sys.exit(1)
    
    
finally:
    
    if con:
        con.close()