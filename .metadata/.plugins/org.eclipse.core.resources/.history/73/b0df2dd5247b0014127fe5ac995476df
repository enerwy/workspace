from PyQt4 import  QtCore, QtGui
from prueba import Ui_Dialog
import prueba
import sys


class interfazPrincipal(QtGui.QDialog, prueba.Ui_Dialog):
    def __init__(self, parent=None):
        super(interfazPrincipal, self).__init__(parent)
        QtGui.QWidget.__init__(self)
        self.setupUi(self)
        self.connectActions()
    

def main():
    app = QtGui.QApplication(sys.argv)
    ventana=prueba()
    ventana.show()
    sys.exit(app.exec_())
if __name__=='__main__':
    main()

